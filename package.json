{
    "name": "Promise-Es6",
    "version": "1.0.0",
    "author": "Bruce yff",
    "license": "MIT",
    "description": "achieve promise using es6",
    "main": "index.js",
    "scripts": {
        "test": "mocha test/index",
        "format": "prettier --write ./**/*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lessYFF/Promise-ES6.git"
    },
    "keywords": [
        "promise"
    ],
    "bugs": {
        "url": "https://github.com/lessYFF/Promise-ES6/issues"
    },
    "homepage": "https://github.com/lessYFF/Promise-ES6#readme",
    "lint-staged": {
        "*.{js,json,ts,css,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -e $GIT_PARAMS"
        }
